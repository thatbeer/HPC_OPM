# Set terminal to png for output
set terminal png size 800,600

# Set output file name
set output 'averaged_plot.png'

# Set graph title and labels
set title 'Vertically Averaged Distribution of u(x)'
set xlabel 'x Position'
set ylabel 'Averaged u(x)'

# Set grid
set grid

# Plot the data
plot 'averaged.dat' using 1:2 with lines title 'u(x)'

# Close the output file
set output
